package main

import (
	// This import statement is essential because calling the init() function defined in the scenarigo package is necessary to register HTTP and gRPC protocols.
	_ "github.com/scenarigo/scenarigo"
	"github.com/scenarigo/scenarigo/plugin"
)

func main() {
	plugin.Register(
		func() []*plugin.Definition {
			return []*plugin.Definition{
				{{- range .Values }}
				plugin.ToDefinition("{{ . }}", {{ . }}),
				{{- end }}
				{{- range .Funcs }}
				plugin.ToDefinition("{{ . }}", {{ . }}),
				{{- end }}
			}
		},
		func() []*plugin.Definition {
			return []*plugin.Definition{
				{{- range .Values }}
				plugin.ToDefinition("{{ . }}", {{ . }}),
				{{- end }}
			}
		},
	)
}
